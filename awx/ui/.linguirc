{"catalogs":[{
   "path": "<rootDir>/locales/{locale}/messages",
   "include": ["<rootDir>"],
   "exclude": ["**/node_modules/**"]
}],
"compileNamespace": "cjs",
"fallbackLocales": { "default": "en"},
"format": "po",
"locales": ["en","es","fr","ko","nl","zh","ja","zu"],
"orderBy": "messageId",
"pseudoLocale": "zu",
"rootDir": "./src",
"runtimeConfigModule": ["@lingui/core", "i18n"],
"sourceLocale": "en"
}
